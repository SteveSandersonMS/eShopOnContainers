<button class="show-concierge" title="Open .NET Concierge" onclick="document.querySelector('.concierge').classList.add('visible');"></button>

<div class="concierge">
    <button class="hide-concierge" title="Close .NET Concierge" onclick="document.querySelector('.concierge').classList.remove('visible')">X</button>
    <iframe src="~/concierge"></iframe>
</div>
